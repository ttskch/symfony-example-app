{% extends 'base.html.twig' %}

{% block javascripts %}
  {{ parent() }}
  {{ encore_entry_script_tags('widgets_multiple-checker') }}
{% endblock %}

{% block content_container %}
  {% include 'widgets/breadcrumb.html.twig' %}

  <div class="container ml-0 px-0">
    <div class="card">
      <div class="card-header">
        {{ 'Menu'|trans }}
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item"><a href="{{ path('user_index') }}" class="d-block"><i class="fa fa-users fa-fw"></i> {{ 'User'|trans }}</a></li>
        <li class="list-group-item"><a href="{{ path('customer_index') }}" class="d-block"><i class="far fa-building fa-fw"></i> {{ 'Customer'|trans }}</a></li>
        <li class="list-group-item"><a href="{{ path('project_index') }}" class="d-block"><i class="fa fa-tasks fa-fw"></i> {{ 'Project'|trans }}</a></li>
      </ul>
    </div>  </div>
{% endblock %}
